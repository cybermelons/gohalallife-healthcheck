{
  "name": "gohalallife-healthcheck",
  "type": "module",
  "version": "1.0.0",
  "description": "Health check and status monitoring for GoHalalLife",
  "scripts": {
    "status": "node cli/index.js status",
    "status:build": "node cli/index.js build",
    "status:prod": "node cli/index.js prod",
    "status:update": "node cli/index.js update",
    "status:graphs": "node cli/index.js graphs",
    "serve": "cd status-page && pnpx serve",
    "status-worker:dev": "cd status-worker && wrangler dev",
    "status-worker:deploy": "cd status-worker && wrangler deploy"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250802.0",
    "wrangler": "^4.27.0"
  }
}